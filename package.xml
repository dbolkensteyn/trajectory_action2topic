<?xml version="1.0"?>
<package>
  <name>trajectory_action2topic</name>
  <version>0.0.0</version>
  <description>Provides an action server that translates between FollowJointTrajectoryAction and sensor_msgs/JointState topics</description>
  <maintainer email="andreas.bihlmaier@gmx.de">Andreas Bihlmaier</maintainer>
  <license>MIT</license>


  <!-- Url tags are optional, but mutiple are allowed, one per tag -->
  <!-- Optional attribute type can be: website, bugtracker, or repository -->
